# Supabase Configuration
# For local development, copy this file to .env and the default values will work
# For cloud Supabase, replace with your project values

# Database Password
POSTGRES_PASSWORD=postgres

# JWT Secret (generate with: openssl rand -base64 32)
JWT_SECRET=your-jwt-secret-here

# API Keys (generate JWT tokens with your secret)
# For local development, use the pre-generated keys in .env
ANON_KEY=your-anon-key-here
SERVICE_ROLE_KEY=your-service-role-key-here

# Frontend Environment Variables
VITE_SUPABASE_URL=http://localhost:8100
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Cesium Ion Token (optional - for 3D terrain)
VITE_CESIUM_ION_TOKEN=
